{
  "publisher": "ntnyq",
  "name": "eslint-command",
  "displayName": "ESLint Command",
  "preview": true,
  "type": "commonjs",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "description": "VSCode support for eslint-plugin-command",
  "keywords": [],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/eslint-command",
  "repository": {
    "type": "github",
    "url": "https://github.com/ntnyq/eslint-command.git"
  },
  "main": "./dist/index.js",
  "icon": "./res/logo.png",
  "scripts": {
    "build": "tsup",
    "dev": "NODE_ENV=development tsup --watch",
    "generate:type": "vscode-ext-gen --output src/meta.ts",
    "lint": "eslint .",
    "pack": "vsce package --no-dependencies",
    "prepare": "husky",
    "publish": "vsce publish --no-dependencies",
    "release": "bumpp && pnpm run publish",
    "typecheck": "tsc --noEmit",
    "vscode:prepublish": "pnpm run build"
  },
  "dependencies": {
    "reactive-vscode": "^0.2.6"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^3.1.1",
    "@ntnyq/prettier-config": "^1.21.3",
    "@types/node": "^22.8.1",
    "@types/vscode": "^1.94.0",
    "@vscode/vsce": "^3.2.1",
    "bumpp": "^9.7.1",
    "eslint": "^9.13.0",
    "husky": "^9.1.6",
    "nano-staged": "^0.8.0",
    "pnpm": "^9.12.3",
    "prettier": "3.3.3",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vscode-ext-gen": "^0.5.0"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "contributes": {
    "commands": [],
    "configuration": {
      "title": "ESLint Command",
      "properties": {
        "eslint-command.enable": {
          "type": "boolean",
          "default": true,
          "description": "Enable extension"
        },
        "eslint-command.languages": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "javascript",
            "javascriptreact",
            "typescript",
            "typescriptreact"
          ],
          "description": "Languages to enable command decorations"
        }
      }
    }
  },
  "categories": [
    "Other"
  ],
  "bugs": {
    "url": "https://github.com/ntnyq/eslint-command/issues/new"
  },
  "engines": {
    "vscode": "^1.94.2"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,json,yml,yaml}": "eslint --fix"
  }
}
