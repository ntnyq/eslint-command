{
  "publisher": "ntnyq",
  "name": "vscode-eslint-command",
  "displayName": "ESLint Command",
  "type": "commonjs",
  "version": "0.0.0",
  "private": true,
  "preview": true,
  "packageManager": "pnpm@9.9.0",
  "description": "VSCode support for eslint-plugin-command",
  "keywords": [],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/vscode-eslint-command",
  "repository": "ntnyq/vscode-eslint-command",
  "main": "./dist/index.js",
  "icon": "./res/logo.png",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "NODE_ENV=development tsup --watch",
    "generate:type": "vscode-ext-gen --output src/meta.ts",
    "lint": "eslint .",
    "pack": "vsce package --no-dependencies",
    "prepare": "husky",
    "publish": "vsce publish --no-dependencies",
    "release": "bumpp && pnpm run publish",
    "typecheck": "tsc --noEmit",
    "vscode:prepublish": "pnpm run build"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^3.0.0-beta.6",
    "@ntnyq/prettier-config": "^1.21.2",
    "@types/node": "^22.5.4",
    "@types/vscode": "^1.93.0",
    "@vscode/vsce": "^3.1.0",
    "bumpp": "^9.5.2",
    "eslint": "^9.9.1",
    "husky": "^9.1.5",
    "nano-staged": "^0.8.0",
    "pnpm": "^9.9.0",
    "prettier": "3.3.3",
    "reactive-vscode": "^0.2.2",
    "tsup": "^8.2.4",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vscode-ext-gen": "^0.4.3"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "contributes": {
    "commands": [],
    "configuration": {
      "title": "Enable extension",
      "properties": {
        "vscode-eslint-command.enable": {
          "type": "boolean",
          "default": true,
          "description": "Enable extension"
        }
      }
    }
  },
  "categories": [
    "Other"
  ],
  "bugs": {
    "url": "https://github.com/ntnyq/vscode-eslint-command/issues/new"
  },
  "engines": {
    "vscode": "^1.93.0"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,json,yml,yaml}": "eslint --fix"
  }
}
